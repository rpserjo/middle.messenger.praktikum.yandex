{
  "env": {
    "browser": true,
    "es2021": true
  },
  //"files": ["src/**/*.ts"],
  "extends": "airbnb-base",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    //"max-params": [1, 1],
    "linebreak-style": "off",
    "no-console": "off",
    "indent": ["error", 4],
    "import/no-extraneous-dependencies": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "max-len": ["warn", 150],
    "no-param-reassign": "off"
  }
}
